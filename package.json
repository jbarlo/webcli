{
  "name": "web-cli",
  "version": "0.1.0",
  "description": "CLI tool for AI-friendly web navigation using text-based browsing",
  "type": "module",
  "bin": {
    "web-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --run"
  },
  "keywords": ["cli", "web", "browser", "automation", "ai"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.30",
    "cheerio": "^1.0.0",
    "commander": "^12.0.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "memfs": "^4.14.0",
    "typescript": "^5.0.0",
    "vitest": "^2.1.8"
  }
}
